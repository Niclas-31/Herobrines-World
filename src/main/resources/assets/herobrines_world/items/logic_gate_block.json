{
    "model": {
        "type": "minecraft:select",
        "block_state_property": "mode",
        "cases": [
            {
                "model": {
                    "type": "minecraft:model",
                    "model": "herobrines_world:block/logic_gate_block_or"
                },
                "when": "or"
            },
            {
                "model": {
                    "type": "minecraft:model",
                    "model": "herobrines_world:block/logic_gate_block_not"
                },
                "when": "not"
            },
            {
                "model": {
                    "type": "minecraft:model",
                    "model": "herobrines_world:block/logic_gate_block_xor"
                },
                "when": "xor"
            }
        ],
        "fallback": {
            "type": "minecraft:model",
            "model": "herobrines_world:block/logic_gate_block_and"
        },
        "property": "minecraft:block_state"
    }
}